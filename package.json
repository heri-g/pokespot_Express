{
  "name": "pokespot-mean-v2",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "del-cli public/javascripts/app && webpack --config webpack.config.dev.js --progress --profile --watch",
    "build:prod": "del-cli public/javascripts/app && ngc -p tsconfig.aot.json && ngc -p tsconfig.aot.json && webpack --config webpack.config.prod.js --progress --profile --bail && del-cli 'public/javascripts/app/**/*.js' 'public/javascripts/app/**/*.js.map' '!public/javascripts/app/bundle.js' '!public/javascripts/app/*.chunk.js' 'src/app/**/*.ngfactory.ts' 'src/app/**/*.shim.ts'"
  },
  "dependencies": {
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "core-js": "^2.4.1",
    "rxjs": "^5.0.1",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2",
    "body-parser": "~1.16.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "hbs": "~4.0.1",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "^2.3.1",
    "@types/core-js": "^0.9.35",
    "@types/node": "^7.0.5",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.4",
    "del-cli": "^0.2.1",
    "html-loader": "^0.4.4",
    "image-webpack-loader": "^3.2.0",
    "raw-loader": "^0.5.1",
    "webpack": "^2.1.0-beta.22",
    "webpack-merge": "^3.0.0"
  }
}
